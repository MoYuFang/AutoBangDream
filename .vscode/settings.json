{
    "code-runner.executorMap": {
        "python": "set PYTHONPATH=$workspaceRoot && python -u $fullFileName"
    },
    "terminal.integrated.env.windows": {
        "PYTHONPATH": "${workspaceFolder}"
    },
    "terminal.integrated.env.osx": {
        "PYTHONPATH": "${workspaceFolder}"
    },
    "terminal.integrated.env.linux": {
        "PYTHONPATH": "${workspaceFolder}"
    },
    "files.associations": {
      "*.cpp": "cpp",
      "*.h": "cpp",
      "*.py": "python",
      "optional": "c",
      "system_error": "c",
      "array": "c",
      "functional": "c",
      "tuple": "c",
      "type_traits": "c",
      "utility": "c"
    },

}